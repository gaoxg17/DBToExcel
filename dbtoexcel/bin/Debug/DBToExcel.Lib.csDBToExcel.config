<csDBToExcel xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <!--
  Sheet: excel sheet
  Sheet.SheetName：excel sheet name
  Sql：sql sentence
  Filed：column
  Filed.FldName：Oracle Filed Name
  Filed.RowName：Excel Filed Name
  DBType：oracle or sqlserver
  ConnStr：connection string
  -->
  <Sheets>
    <Sheet SheetName="Sheet1Name">    
      <Sql>select COL_INT,COL_VARCHAR,COL_DATE,COL_CHAR,COL_LONG from TEST_BYGAOXIONG20181108</Sql>
      <Fileds>
        <Filed FldName="COL_INT" RowName="INT..." />
        <Filed FldName="COL_VARCHAR" RowName="VARCHAR..." />
        <Filed FldName="COL_DATE" RowName="DATE..." />
        <Filed FldName="COL_CHAR" RowName="CHR..." />
        <Filed FldName="COL_LONG" RowName="LONGxx" />
        <Filed FldName="unknown" RowName="???" />
      </Fileds>
    </Sheet>
    <Sheet SheetName="Sheet2Name">
      <Sql>select COL_INT,COL_VARCHAR,COL_DATE,COL_CHAR,COL_LONG from TEST_BYGAOXIONG20181108</Sql>
      <Fileds>
        <Filed FldName="COL_INT" RowName="INT..." />
        <Filed FldName="COL_VARCHAR" RowName="VARCHAR..." />
        <Filed FldName="COL_DATE" RowName="DATE..." />
        <Filed FldName="COL_CHAR" RowName="CHR..." />
        <Filed FldName="COL_LONG" RowName="LONGxx" />
        <Filed FldName="unknown" RowName="???" />
      </Fileds>
    </Sheet>
  </Sheets>
  <DBType>sqlserver</DBType>
  <ConnStr>data source=.;initial catalog=Test20181108;user id=sa;password=123456;</ConnStr>
</csDBToExcel>
